  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <OutDir>$(ProjectDir)..\..\Game\</OutDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
